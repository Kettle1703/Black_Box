<Page x:Class="проект.experience"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:проект"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="experience" KeyUp="Page_KeyUp">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="opyty_bek.jpg"></ImageBrush>
        </Grid.Background>
        <StackPanel>
        <Label Content="Введите входные данные:" FontSize="20" HorizontalAlignment="Center" Margin="0 140 0 10" VerticalAlignment="Top"/>
        <TextBox x:Name="input" FontSize="20" HorizontalAlignment="Center"  TextWrapping="Wrap" Text="" Width="417" Height="39"/>
        <Label Content="Выходные данные:" FontSize="20" HorizontalAlignment="Center" Margin="0 10 0 10"/>
        <TextBlock x:Name="output" FontSize="20" HorizontalAlignment="Center"  TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="417" Height="37" TextAlignment="Center"/>
        </StackPanel>
    </Grid>
</Page>
