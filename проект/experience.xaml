        <Label Content="Выходные данные:" FontSize="20" HorizontalAlignment="Left" Margin="39,116,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="output" FontSize="20" HorizontalAlignment="Left" Margin="39,158,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="417" Height="37"/>

    </Grid>
</Page>
